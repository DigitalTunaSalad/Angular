{
  "name": "digitaltunasalad-template",
  "version": "0.1.0",
  "description": "Angular boilerplate code using webpack",
  "scripts": {
    "test": "karma start config/karma.conf.js",
    "build:webpack": "tsc -p tsconfig.webpack.json",
    "build:app": "node node_modules/webpack/bin/webpack.js",
    "build:vendor": "node node_modules/webpack/bin/webpack.js --config webpack.vendor.config.js",
    "build": "npm run build:vendor && npm run build:app",
    "build:app-prod": "node node_modules/webpack/bin/webpack.js --env.prod",
    "build:vendor-prod": "node node_modules/webpack/bin/webpack.js --config webpack.vendor.config.js --env.prod",
    "build:prod": "npm run build:vendor-prod && npm run build:app-prod"
  },
  "author": "Digital Tuna Salad",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@angular/animations": "^5.2.3",
    "@angular/common": "^5.2.3",
    "@angular/compiler": "^5.2.3",
    "@angular/compiler-cli": "^5.2.3",
    "@angular/core": "^5.2.3",
    "@angular/forms": "^5.2.3",
    "@angular/http": "^5.2.3",
    "@angular/platform-browser": "^5.2.3",
    "@angular/platform-browser-dynamic": "^5.2.3",
    "@angular/platform-server": "^5.2.3",
    "@angular/router": "^5.2.3",
    "@ngtools/webpack": "^1.9.7",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/jasmine": "^2.8.6",
    "@types/node": "8.0.19",
    "@types/webpack": "^3.8.5",
    "@types/webpack-env": "^1.13.5",
    "@types/webpack-merge": "^4.1.1",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "aspnet-prerendering": "^3.0.1",
    "aspnet-webpack": "^2.0.3",
    "awesome-typescript-loader": "^3.4.1",
    "css-loader": "^0.28.9",
    "es6-shim": "^0.35.3",
    "event-source-polyfill": "0.0.12",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.5",
    "jasmine-core": "^2.9.1",
    "karma": "^2.0.0",
    "karma-jasmine": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.9",
    "node-sass": "^4.7.2",
    "phantomjs-prebuilt": "^2.1.16",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "typescript": "^2.7.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1",
    "zone.js": "^0.8.20"
  }
}
